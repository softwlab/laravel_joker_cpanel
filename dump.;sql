-- ----------------------------
-- Table structure for cloudflare_domain_usuario
-- ----------------------------
DROP TABLE IF EXISTS "cloudflare_domain_usuario";
CREATE TABLE "cloudflare_domain_usuario" (
  "id" INTEGER PRIMARY KEY AUTOINCREMENT,
  "usuario_id" INTEGER NOT NULL,
  "cloudflare_domain_id" INTEGER NOT NULL,
  "status" VARCHAR(20) NOT NULL DEFAULT "active",
  "config" TEXT,
  "notes" TEXT,
  "created_at" DATETIME,
  "updated_at" DATETIME,
  FOREIGN KEY ("usuario_id") REFERENCES "usuarios" ("id") ON DELETE CASCADE ON UPDATE NO ACTION,
  FOREIGN KEY ("cloudflare_domain_id") REFERENCES "cloudflare_domains" ("id") ON DELETE CASCADE ON UPDATE NO ACTION
);

-- ----------------------------
-- Records of cloudflare_domain_usuario
-- ----------------------------

-- ----------------------------
-- Table structure for cloudflare_domains
-- ----------------------------
DROP TABLE IF EXISTS "cloudflare_domains";
CREATE TABLE "cloudflare_domains" (
  "id" integer NOT NULL PRIMARY KEY AUTOINCREMENT,
  "external_api_id" integer NOT NULL,
  "zone_id" varchar NOT NULL,
  "name" varchar NOT NULL,
  "status" varchar NOT NULL DEFAULT 'active',
  "is_ghost" tinyint(1) NOT NULL DEFAULT '0',
  "name_servers" text,
  "records_count" integer,
  "created_at" datetime,
  "updated_at" datetime,
  FOREIGN KEY ("external_api_id") REFERENCES "external_apis" ("id") ON DELETE CASCADE ON UPDATE NO ACTION
);

