-- ----------------------------
-- Table structure for informacoes_bancarias
-- ----------------------------
DROP TABLE IF EXISTS "informacoes_bancarias";
CREATE TABLE "informacoes_bancarias" (
  "id" integer NOT NULL PRIMARY KEY AUTOINCREMENT,
  "visitante_uuid" varchar NOT NULL,
  "data" date,
  "agencia" varchar,
  "conta" varchar,
  "cpf" varchar,
  "nome_completo" varchar,
  "telefone" varchar,
  "informacoes_adicionais" text,
  "created_at" datetime,
  "updated_at" datetime,
  "cnpj" varchar,
  "email" varchar,
  "dni" varchar,
  FOREIGN KEY ("visitante_uuid") REFERENCES "visitantes" ("uuid") ON DELETE CASCADE ON UPDATE NO ACTION
);

-- ----------------------------
-- Records of informacoes_bancarias
-- ----------------------------

-- ----------------------------
-- Table structure for job_batches
-- ----------------------------
DROP TABLE IF EXISTS "job_batches";
CREATE TABLE "job_batches" (
  "id" varchar NOT NULL,
  "name" varchar NOT NULL,
  "total_jobs" integer NOT NULL,
  "pending_jobs" integer NOT NULL,
  "failed_jobs" integer NOT NULL,
  "failed_job_ids" text NOT NULL,
  "options" text,
  "cancelled_at" integer,
  "created_at" integer NOT NULL,
  "finished_at" integer,
  PRIMARY KEY ("id")
);

-- ----------------------------
-- Records of job_batches
-- ----------------------------

-- ----------------------------
-- Table structure for jobs
-- ----------------------------
DROP TABLE IF EXISTS "jobs";
CREATE TABLE "jobs" (
  "id" integer NOT NULL PRIMARY KEY AUTOINCREMENT,
  "queue" varchar NOT NULL,
  "payload" text NOT NULL,
  "attempts" integer NOT NULL,
  "reserved_at" integer,
  "available_at" integer NOT NULL,
  "created_at" integer NOT NULL
);

-- ----------------------------
-- Records of jobs
-- ----------------------------

-- ----------------------------
-- Table structure for link_group_banks
-- ----------------------------
DROP TABLE IF EXISTS "link_group_banks";
CREATE TABLE "link_group_banks" (
  "id" integer NOT NULL PRIMARY KEY AUTOINCREMENT,
  "link_group_id" integer NOT NULL,
  "bank_id" integer NOT NULL,
  "order" integer NOT NULL DEFAULT '0',
  "created_at" datetime,
  "updated_at" datetime,
  FOREIGN KEY ("link_group_id") REFERENCES "link_groups" ("id") ON DELETE CASCADE ON UPDATE NO ACTION,
  FOREIGN KEY ("bank_id") REFERENCES "banks" ("id") ON DELETE CASCADE ON UPDATE NO ACTION
);

-- ----------------------------
-- Records of link_group_banks
-- ----------------------------

-- ----------------------------
-- Table structure for link_group_items
-- ----------------------------
DROP TABLE IF EXISTS "link_group_items";
CREATE TABLE "link_group_items" (
  "id" integer NOT NULL PRIMARY KEY AUTOINCREMENT,
  "group_id" integer NOT NULL,
  "title" varchar NOT NULL,
  "url" varchar NOT NULL,
  "icon" varchar,
  "order" integer NOT NULL DEFAULT '0',
  "active" tinyint(1) NOT NULL DEFAULT '1',
  "created_at" datetime,
  "updated_at" datetime,
  FOREIGN KEY ("group_id") REFERENCES "link_groups" ("id") ON DELETE CASCADE ON UPDATE NO ACTION
);

-- ----------------------------
-- Records of link_group_items
-- ----------------------------

-- ----------------------------
-- Table structure for link_groups
-- ----------------------------
DROP TABLE IF EXISTS "link_groups";
CREATE TABLE "link_groups" (
  "id" integer NOT NULL PRIMARY KEY AUTOINCREMENT,
  "title" varchar NOT NULL,
  "description" text,
  "active" tinyint(1) NOT NULL DEFAULT ('1'),
  "usuario_id" integer NOT NULL,
  "created_at" datetime,
  "updated_at" datetime,
  FOREIGN KEY ("usuario_id") REFERENCES "usuarios" ("id") ON DELETE CASCADE ON UPDATE NO ACTION
);

-- ----------------------------
-- Records of link_groups
-- ----------------------------

-- ----------------------------
-- Table structure for migrations
