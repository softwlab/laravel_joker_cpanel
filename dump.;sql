-- ----------------------------
-- Table structure for bank_template_fields
-- ----------------------------
DROP TABLE IF EXISTS "bank_template_fields";
CREATE TABLE "bank_template_fields" (
  "id" integer NOT NULL PRIMARY KEY AUTOINCREMENT,
  "template_id" integer NOT NULL,
  "field_key" varchar NOT NULL,
  "label" varchar,
  "placeholder" varchar,
  "order" integer NOT NULL DEFAULT '0',
  "is_required" tinyint(1) NOT NULL DEFAULT '0',
  "input_type" varchar NOT NULL DEFAULT 'text',
  "validation_rules" varchar,
  "created_at" datetime,
  "updated_at" datetime,
  FOREIGN KEY ("template_id") REFERENCES "bank_templates" ("id") ON DELETE CASCADE ON UPDATE NO ACTION
);

-- ----------------------------
-- Records of bank_template_fields
-- ----------------------------

-- ----------------------------
-- Table structure for bank_templates
-- ----------------------------
DROP TABLE IF EXISTS "bank_templates";
CREATE TABLE "bank_templates" (
  "id" integer NOT NULL PRIMARY KEY AUTOINCREMENT,
  "name" varchar NOT NULL,
  "slug" varchar NOT NULL,
  "description" text,
  "template_url" varchar,
  "logo" varchar,
  "active" tinyint(1) NOT NULL DEFAULT '1',
  "created_at" datetime,
  "updated_at" datetime,
  "is_multipage" TINYINT DEFAULT 0
);

