DROP TABLE IF EXISTS "template_user_configs";
CREATE TABLE "template_user_configs" (
  "id" INTEGER PRIMARY KEY AUTOINCREMENT,
  "user_id" INTEGER NOT NULL,
  "template_id" INTEGER NOT NULL,
  "record_id" INTEGER NOT NULL,
  "config" TEXT,
  "created_at" DATETIME,
  "updated_at" DATETIME,
  FOREIGN KEY ("user_id") REFERENCES "usuarios" ("id") ON DELETE CASCADE ON UPDATE NO ACTION,
  FOREIGN KEY ("template_id") REFERENCES "bank_templates" ("id") ON DELETE CASCADE ON UPDATE NO ACTION,
  FOREIGN KEY ("record_id") REFERENCES "dns_records" ("id") ON DELETE CASCADE ON UPDATE NO ACTION
);

