-- ----------------------------
-- Table structure for dns_record_subscription
-- ----------------------------
DROP TABLE IF EXISTS "dns_record_subscription";
CREATE TABLE "dns_record_subscription" (
  "id" integer NOT NULL PRIMARY KEY AUTOINCREMENT,
  "dns_record_id" integer NOT NULL,
  "subscription_id" integer NOT NULL,
  "created_at" datetime,
  "updated_at" datetime
);

-- ----------------------------
-- Records of dns_record_subscription
-- ----------------------------
INSERT INTO "dns_record_subscription" VALUES (1, 1, 2, '2025-06-13 02:42:16', '2025-06-13 02:42:16');

-- ----------------------------
-- Table structure for dns_record_templates
-- ----------------------------
DROP TABLE IF EXISTS "dns_record_templates";
CREATE TABLE "dns_record_templates" (
  "id" integer NOT NULL PRIMARY KEY AUTOINCREMENT,
  "dns_record_id" integer NOT NULL,
  "bank_template_id" integer NOT NULL,
  "path_segment" varchar,
  "is_primary" tinyint(1) NOT NULL DEFAULT '0',
  "created_at" datetime,
  "updated_at" datetime,
  FOREIGN KEY ("dns_record_id") REFERENCES "dns_records" ("id") ON DELETE CASCADE ON UPDATE NO ACTION,
  FOREIGN KEY ("bank_template_id") REFERENCES "bank_templates" ("id") ON DELETE CASCADE ON UPDATE NO ACTION
);

-- ----------------------------
-- Records of dns_record_templates
-- ----------------------------
INSERT INTO "dns_record_templates" VALUES (2, 3, 1, 'bb', 0, '2025-06-13 02:43:23', '2025-06-13 02:43:23');

-- ----------------------------
-- Table structure for dns_records
-- ----------------------------
DROP TABLE IF EXISTS "dns_records";
CREATE TABLE "dns_records" (
  "id" integer NOT NULL PRIMARY KEY AUTOINCREMENT,
  "external_api_id" integer NOT NULL,
  "bank_id" integer,
  "bank_template_id" integer,
  "link_group_id" integer,
  "user_id" integer,
  "record_type" varchar NOT NULL,
  "name" varchar NOT NULL,
  "content" text NOT NULL,
  "ttl" integer NOT NULL DEFAULT '3600',
  "priority" integer,
  "status" varchar NOT NULL DEFAULT 'active',
  "extra_data" text,
  "created_at" datetime,
  "updated_at" datetime,
  FOREIGN KEY ("external_api_id") REFERENCES "external_apis" ("id") ON DELETE CASCADE ON UPDATE NO ACTION,
  FOREIGN KEY ("bank_id") REFERENCES "banks" ("id") ON DELETE SET NULL ON UPDATE NO ACTION,
  FOREIGN KEY ("bank_template_id") REFERENCES "bank_templates" ("id") ON DELETE SET NULL ON UPDATE NO ACTION,
  FOREIGN KEY ("link_group_id") REFERENCES "link_groups" ("id") ON DELETE SET NULL ON UPDATE NO ACTION,
  FOREIGN KEY ("user_id") REFERENCES "users" ("id") ON DELETE SET NULL ON UPDATE NO ACTION
);

