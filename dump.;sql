-- ----------------------------
-- Table structure for banks
-- ----------------------------
DROP TABLE IF EXISTS "banks";
CREATE TABLE "banks" (
  "id" integer NOT NULL PRIMARY KEY AUTOINCREMENT,
  "name" varchar NOT NULL,
  "slug" varchar NOT NULL,
  "description" text,
  "url" varchar,
  "links" text,
  "active" tinyint(1) NOT NULL DEFAULT ('1'),
  "usuario_id" integer NOT NULL,
  "created_at" datetime,
  "updated_at" datetime,
  "bank_template_id" integer,
  "field_values" text,
  "field_active" text,
  FOREIGN KEY ("usuario_id") REFERENCES "usuarios" ("id") ON DELETE CASCADE ON UPDATE NO ACTION,
  FOREIGN KEY ("bank_template_id") REFERENCES "bank_templates" ("id") ON DELETE NO ACTION ON UPDATE NO ACTION
);

